<view class="weui-cells__group weui-cells__group_form">

    <form bindsubmit="formSubmit" bindreset="formReset">

        <view class="weui-cells weui-cells_form">

            <view class="weui-cell weui-cell_active weui-cell_disabled">
                <view class="weui-cell__hd">
                    <label class="weui-label">姓名:</label>
                </view>
                <view class="weui-cell__bd">
                    <input name="input_name" class="weui-input" placeholder="{{placeholder_name}}" value="{{placeholder_name}}" />
                </view>
            </view>


            <view class="weui-cell weui-cell_active weui-cell_disabled">
                <view class="weui-cell__hd">
                    <label class="weui-label">身份证号:</label>
                </view>
                <view class="weui-cell__bd">
                    <input name="input_ID" class="weui-input" placeholder="{{placeholder_id}}" value="{{placeholder_id}}"  />
                </view>


            </view>

            <view class="weui-cell weui-cell_active weui-cell_disabled">
                <view class="weui-cell__hd">
                    <label class="weui-label">地址:</label>
                </view>
                <view class="weui-cell__bd">
                    <input name="input_addr" class="weui-input" placeholder="{{placeholder_addr}}" value="{{placeholder_addr}}"  />
                </view>


            </view>



            <view class="weui-cell weui-cell_active weui-cell_disabled">
                <view class="weui-cell__hd">
                    <label class="weui-label">性别:</label>
                </view>
                <view class="weui-cell__bd">
                    <input name="input_gender" class="weui-input" placeholder="{{placeholder_gender}}" value="{{placeholder_gender}}"  disabled/>
                </view>

            </view>


