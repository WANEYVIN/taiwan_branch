                                        <view><button class="buttonConfig yellowButton" id="{{item.task_id}}" bindtap="editing">{{editing}}</button></view>
                                        <view><button class="buttonConfig redButton" id="{{item.task_id}}" bindtap="dropProject">{{dropProject}}</button></view>
                                    </view>

