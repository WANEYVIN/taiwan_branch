                    <picker mode="date" value="{{member_birthday}}" start="1900-09-01" end="2005-09-01" bindchange="birthdayChange">
                        <a class="">{{member_birthday}}</a>
                    </picker>

                </view>

            </view>


            <view class="weui-cell weui-cell_active weui-cell_disabled">
                <view class="weui-cell__hd">
                    <label class="weui-label">{{label_nationality}}</label>
                </view>
                <view class="weui-cell__bd">
                    <input name="input_nationality" class="weui-input" placeholder="{{placeholder_nationality}}" value="{{placeholder_nationality}}"  />
                </view>


            </view>

